{
  "name": "volkai-hr-edu-backend",
  "version": "1.0.0",
  "description": "Volkai HR Education Platform - Comprehensive backend API with Mock Interview and Resume Builder integration",
  "author": "Volkai Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:watch": "nest build --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathIgnorePatterns=.*\\.e2e-spec\\.ts$",
    "test:integration": "jest --testPathPattern=.*\\.integration\\.spec\\.ts$",
    "test:security": "ts-node src/scripts/run-security-tests.ts",
    "test:performance": "jest --config ./test/jest-performance.json",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security",
    "test:health": "curl -f http://localhost:3000/health || exit 1",
    "test:smoke": "jest --config ./test/jest-smoke.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm2": "node --loader ts-node/esm ./node_modules/typeorm/cli.js -d src/database/data-source.ts",
    "db:migration:generate": "npm run typeorm2 -- migration:generate src/migrations/InitSchema",
    "db:migration:run": "npm run typeorm2 -- migration:run",
    "db:migration:revert": "npm run typeorm2 -- migration:revert",
    "db:show": "npm run typeorm2 -- schema:log",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/database/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/data-source.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/database/data-source.ts",
    "seed:run": "ts-node src/database/seeds/run-seeds.ts",
    "seed:revert": "ts-node src/database/seeds/revert-seeds.ts",
    "db:setup": "npm run migration:run && npm run seed:run",
    "db:reset": "npm run migration:revert && npm run migration:run && npm run seed:run",
    "test:db:setup": "NODE_ENV=test npm run migration:run && NODE_ENV=test npm run seed:run",
    "test:db:reset": "NODE_ENV=test npm run migration:revert && NODE_ENV=test npm run migration:run",
    "docker:build": "docker build -t volkai-backend .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f api",
    "env:validate": "ts-node src/scripts/validate-environment.ts",
    "config:check": "ts-node src/scripts/check-configuration.ts",
    "audit": "npm audit --audit-level moderate",
    "vulnerability:check": "npm audit --audit-level high",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.441.0",
    "@aws-sdk/lib-storage": "^3.441.0",
    "@aws-sdk/s3-request-presigner": "^3.441.0",
    "@elastic/elasticsearch": "^9.1.1",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.4.20",
    "@sendgrid/mail": "^7.7.0",
    "argon2": "^0.44.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.4",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-store": "^3.0.1",
    "clamscan": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "html-escaper": "^3.0.3",
    "isomorphic-dompurify": "^2.30.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nest-winston": "^1.9.4",
    "nodemailer": "^6.9.7",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "razorpay": "^2.9.6",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.6",
    "socket.io": "^4.8.1",
    "stripe": "^19.1.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/html-escaper": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.198",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.6.3",
    "@types/nodemailer": "^6.4.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1",
      "^@database/(.*)$": "<rootDir>/database/$1"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
